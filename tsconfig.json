{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "outDir": "build",
    "allowJs": true,
    "downlevelIteration": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
